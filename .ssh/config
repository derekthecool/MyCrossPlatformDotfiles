# ssh chooses the first matching host
HOST 192.168.1.*
     USER root
     RequestTTY yes
     RemoteCommand tmux new -A

HOST ogd*
     USER dlomax
     RequestTTY yes
     RemoteCommand tmux new -A

HOST github RSA key
HostName github.com
IdentityFile ~/.ssh/id_rsa
User git
IdentitiesOnly yes

HOST github ED25519 key
HostName github.com
IdentityFile ~/.ssh/id_ed25519
User git
IdentitiesOnly yes

HOST ssh.dev.azure.com vs-ssh.visualstudio.com
IdentityFile ~/.ssh/id_rsa_work
User git
IdentitiesOnly yes

HOST *
     USER dlomax
